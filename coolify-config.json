{"version": "1.0","name": "Claude Code UI with Cline CLI & MCP TaskMaster AI","description": "Web-based UI for Claude Code CLI with integrated Cline CLI as main LLM and MCP TaskMaster AI for project management","documentation": "https://claudecodeui.siteboon.ai","repository": "https://github.com/SuperPauly/claudecodeui","docker": {"dockerfile": "Dockerfile","buildArgs": [],"ports": [{"container": 3000,"host": 3000,"description": "Claude Code UI Web Interface"},{"container": 3001,"host": 3001,"description": "MCP TaskMaster AI API (optional)","required": false}]},"environment": {"required": [{"key": "CLINE_API_KEY","description": "Cline CLI API key for LLM operations","example": "sk-...","required": true},{"key": "JWT_SECRET","description": "Secret key for JWT token generation","example": "your-secret-key-min-32-chars","required": true}],"optional": [{"key": "CLINE_MODEL","description": "Cline model to use","example": "gpt-4","default": "gpt-4"},{"key": "NODE_ENV","description": "Node environment","example": "production","default": "production"},{"key": "LOG_LEVEL","description": "Logging level","example": "info","default": "info"},{"key": "MCP_TASKMASTER_ENABLED","description": "Enable MCP TaskMaster AI integration","example": "true","default": "true"},{"key": "TASKMASTER_API_KEY","description": "MCP TaskMaster AI API key","example": "taskmaster-...","required": false},{"key": "ENABLE_FILE_UPLOAD","description": "Enable file upload feature","default": "true"},{"key": "ENABLE_GIT_INTEGRATION","description": "Enable git integration","default": "true"},{"key": "ENABLE_TERMINAL","description": "Enable terminal access","default": "true"},{"key": "MAX_FILE_SIZE","description": "Maximum file upload size in bytes","default": "52428800","example": "52428800"},{"key": "SESSION_TIMEOUT","description": "Session timeout in milliseconds","default": "3600000"}]},"volumes": [{"container": "/home/appuser/.cline/projects","description": "Cline CLI project sessions","required": true},{